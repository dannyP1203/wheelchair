<?xml version="1.0"?>
<robot name="wchair">
<!-- Rosso:= Asse x	 Verde:= Asse y	 Blue := Asse z -->
<!-- ##################################################################### -->
	<!--_____________________BASE LINK_____________________________________-->
	<link name="base_link">
		<visual>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry><box size="0.75 0.5 0.30"/></geometry>
			<material name="blue"><color rgba="0 0 0.6 1"/></material>
		</visual>
		<!-- Base collision -->
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0" />
			<geometry><box size="0.75 0.5 0.30"/></geometry>
		</collision>
		<inertial>
			<mass value="5"/>
			<inertia ixx="0.13" ixy="0.0" ixz="0.0" iyy="0.21" iyz="0.0" izz="0.13"/>
		</inertial>
	</link>
	<gazebo reference="base_link">
		<material>Gazebo/SkyBlue</material>
		<!-- <turnGravityOff>false</turnGravityOff> -->
	</gazebo>
<!-- ##################################################################### -->
	<!--_____________________RIGHT TOP______________________________________-->
	<link name="right_top">
		<visual>
			<origin rpy="0 0  0 " xyz="0 0 0"/>
			<geometry><cylinder length="0.2" radius="0.05"/></geometry>
			<material name="white"><color rgba="1 1 1 1"/></material>
		</visual>
		<collision>
			<origin rpy="0 0 0" xyz="0 0 0"/>
			<geometry><cylinder length="0.2" radius="0.05"/></geometry>
		</collision>
		<inertial>
			<mass value="0.5"/>
			<inertia ixx="0.0001" ixy="0.0" ixz="0.0" iyy="0.0001" iyz="0.0" izz="0.0001"/>
		</inertial>
	</link>
	<gazebo reference="right_top">
		<material>Gazebo/White</material>
		<!--<turnGravityOff>false</turnGravityOff>-->
	</gazebo>
	<joint name="right_top_joint" type="revolute">
		<parent link="base_link"/>
		<child link="right_top"/>
		<origin rpy="0 0 0" xyz="0.375 -0.275 0"/>
		<axis xyz="0 0 1"/>
	</joint>
	<!-- Transmission is important to link the joints and the controller -->
	<transmission name="right_top_joint_trans">
		<type>transmission_interface/SimpleTransmission</type>
		<joint name="right_top_joint"/>
		<actuator name="right_top_joint_motor">
			<hardwareInterface>EffortJointInterface</hardwareInterface>
			<mechanicalReduction>1</mechanicalReduction>
		</actuator>
	</transmission>
